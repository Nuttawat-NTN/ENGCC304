#include <stdio.h>

int main() {
    int A = 0 ;
    printf( "your Number") ;
    scanf( "%d", &A ) ;
    if( A % 2 != 0 ) {
        for(int i = 1 ; i <= A ; i += 2 ){
            printf( "%d ", i ) ;
        }//end for
        
    } else {
        for(int i = A ; i >= 0  ; i -= 2 ){
            printf( "%d ", i ) ;
        }//end for
    }//end if
    return 0 ;
}//end function
